## python code
name  = 'turnstile-110528.txt'
old = open(name, 'r') 
new = open('01_updated_'+name, 'w')
read = csv.reader(old,delimiter=',')
writer = csv.writer(new,delimiter=',')

for row in read :
    i=3

    while i<len(row):
        ru = [row[0],row[1],row[2]]
        for k in range(0,5):
            ru.append(row[i+k])
        writer.writerow(ru)
        i = i+5
